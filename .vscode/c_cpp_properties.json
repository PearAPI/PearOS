{
    "env": {
        "includes": [
            "${workspaceFolder}/kernel/include/private/",
            "${workspaceFolder}/kernel/include/public/"
        ]
    },

    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${includes}"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c99",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-clang-x64",
            "browse": {
                "path": [
                    "${includes}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
        }
    ],
    "version": 4
}